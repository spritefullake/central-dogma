{
  "name": "genetics",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _",
    "clean": "bsb -clean-world",
    "test": "jest",
    "watch": "webpack --watch",
    "re:build": "bsb -make-world -clean-world",
    "re:watch": "bsb -make-world -clean-world -w",
    "dev:server": "moduleserve ./ --port 8000",
    "dev:serve": "webpack-dev-server --open",
    "exp:serve": "node server.js"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-platform": "^7.2.2",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "moduleserve": "^0.9.0",
    "raw-loader": "^4.0.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "bs-css-dom": "^1.2.0",
    "bs-fetch": "^0.5.2",
    "reason-react": "^0.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(tsv|.txt)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
